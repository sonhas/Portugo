    leia (n1)
    escreva ("digite a segunda nota: ")
    leia (n2)
    escreva ("digite a terceira nota: ")
    leia (n3)
    escreva ("digite a quarta nota: ")
    leia (n4)

    media=(n1+n2+n3+n4)/ 4
    escreva ("A media eÃ© obtida:", media)

    se(media >=80){
      escreva ("Aluno aprovado")
    } senao {
      escreva ("Aluno reprovado")
    }
    
  }
